{
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "App\\Services\\": "src/Services/",
            "App\\Classes\\": "src/Classes/",
            "App\\Classes\\User\\": "src/Classes/User/",
            "App\\Classes\\Security\\": "src/Classes/Security/",
            "App\\Classes\\Validation\\": "src/Classes/Validation/",
            "App\\Classes\\Cache\\": "src/Classes/Cache/",
            "App\\Classes\\Auth\\": "src/Classes/Auth/",
            "App\\Traits\\": "src/Traits/",
            "App\\Components\\": "public/Components/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "phpmailer/phpmailer": "^6.9",
        "firebase/php-jwt": "^6.11",
        "predis/predis": "^2.3",
        "guzzlehttp/guzzle": "^7.9",
        "symfony/yaml": "^6.4",
        "symfony/translation": "^6.4",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
